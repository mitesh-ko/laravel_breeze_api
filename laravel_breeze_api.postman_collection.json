	"auth": {
		"type": "bearer",
		"bearer": [
			{
				"key": "token",
				"value": "{{token}}",
				"type": "string"
			}
		]
	},
	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"pm.sendRequest({",
					"    url: `${pm.collectionVariables.get('origin')}/sanctum/csrf-cookie`,",
					"    method: 'GET'",
					"}, function (error, response, {cookies}){",
					"    if(!error) {",
					"        pm.collectionVariables.set('xsrf-token', cookies.get('XSRF-TOKEN'))",
					"    }",
					"});"
				]
			}
		},
